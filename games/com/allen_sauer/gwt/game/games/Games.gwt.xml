  Copyright 2009 Fred Sauer
-->
<module rename-to="games">

  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name='com.google.gwt.user.User' />

  <!-- Inherit game stuff.                      -->
  <inherits name='com.allen_sauer.gwt.game.Game' />

  <!-- Inherit Game Modules.                                           -->
  <inherits name='com.allen_sauer.gwt.game.hornetblast.HornetBlastGame' />
  <inherits name='com.allen_sauer.gwt.game.space.SpaceGame' />

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.allen_sauer.gwt.game.games.client.Games' />

</module>
